  "repository": {
    "type": "git",
    "url": "https://github.com/mikemey/cellcomm.git"
  },
  "author": "mikemey",
  "license": "MIT",
  "dependencies": {
    "express": "4.17.1",
    "moment": "2.29.4",
    "mongodb": "3.6.4",
    "morgan": "1.10.0",
    "pug": "3.0.2"
  },
  "devDependencies": {
    "chai": "4.3.1",
    "mocha": "8.3.0",
    "nodemon": "2.0.7",
    "standard": "16.0.3",
    "supertest": "6.1.3"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "$"
    ]
