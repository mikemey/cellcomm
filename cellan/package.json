{
  "name": "cell-analysis",
  "version": "0.0.1",
  "description": "Cell encoding analysis backend",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "node index.js",
    "lint": "standard",
    "specs": "mocha 'test/**/**.spec.js'",
    "~specs": "nodemon -x 'npm run specs -s'",
    "test": "npm run lint -s && npm run specs -s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikemey/cellcomm.git"
  },
  "author": "mikemey",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "mongodb": "^3.5.9"
  },
  "devDependencies": {
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "standard": "^14.3.4",
    "supertest": "^4.0.2"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "$"
    ]
  }
}
